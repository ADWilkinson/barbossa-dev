{
  "owner": "your-github-username",
  "issue_tracker": {
    "type": "linear",
    "linear": {
      "team_key": "MUS",
      "backlog_state": "Backlog"
    }
  },
  "repositories": [
    {
      "name": "my-app",
      "url": "https://github.com/your-github-username/my-app.git",
      "package_manager": "npm"
    }
  ],
  "settings": {
    "telemetry": true,
    "engineer": {
      "enabled": true
    },
    "tech_lead": {
      "enabled": true,
      "auto_merge": true
    },
    "discovery": {
      "enabled": true,
      "max_backlog_issues": 20
    },
    "product_manager": {
      "enabled": true,
      "max_issues_per_run": 3
    }
  }
}
